    <type>
        <leftType>OrgUnit</leftType>
        <rightType>Person</rightType>
        <rightwardType>isMemberOfOrgUnit</rightwardType>
        <leftwardType>isOrgUnitOfPerson</leftwardType>
        <leftCardinality>
            <min>0</min>
        </leftCardinality>
        <rightCardinality>
            <min>0</min>
        </rightCardinality>
    </type>
    
